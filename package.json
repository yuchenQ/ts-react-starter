{
  "name": "ts-react-starter",
  "version": "1.0.0",
  "description": "The React boilerplate with webpack, babel, typescript, jest and enzyme.",
  "main": "src/index.tsx",
  "scripts": {
    "build": "webpack --config webpack.config.prod.babel.js",
    "start": "webpack-dev-server --config webpack.config.dev.babel.js",
    "test": "jest --passWithNoTests",
    "format": "pretty-quick",
    "lint": "concurrently npm:lint:*",
    "lint:es": "eslint src --ext .js,.ts,.tsx,.jsx --quiet --fix",
    "lint:css": "stylelint './src/**/*.tsx'"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "typescript",
    "jest",
    "enzyme",
    "commit-lint",
    "prettier",
    "eslint"
  ],
  "author": "Yuchen Qiao <qiaoycitcareer@gmail.com> (https://www.linkedin.com/in/yuchen-qiao-761450a5/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeekEast/create-react-ts.git"
  },
  "bugs": {
    "url": "https://github.com/GeekEast/create-react-ts/issues"
  },
  "homepage": "https://github.com/GeekEast/create-react-ts#readme",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.7.7",
    "@types/styled-components": "^4.4.2",
    "concurrently": "^5.0.2",
    "core-js": "^3.6.2",
    "jest-styled-components": "^6.3.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "regenerator-runtime": "^0.13.3",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.25",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node": "^13.1.5",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.41.2",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.9.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
